1 VERSIONINFO
 FILEVERSION ${VERSION_MAJOR}, ${VERSION_MINOR}, ${VERSION_ALTER}, ${VERSION_BUILD}
 FILEFLAGSMASK 0x17L
#ifdef _DEBUG
 FILEFLAGS 0x1L
#else
 FILEFLAGS 0x0L
#endif
 FILEOS 0x4L
 FILETYPE 0x1L
 FILESUBTYPE 0x0L
BEGIN
    BLOCK "StringFileInfo"
    BEGIN
        BLOCK "040904b0"
        BEGIN
            VALUE "FileDescription", "Jellyfin for Windows"
            VALUE "LegalCopyright", "dragonflyee (C) Copyright 2023"
            VALUE "OriginalFilename", "Switchfin.exe"
            VALUE "ProductVersion", "${VERSION_MAJOR}.${VERSION_MINOR}.${VERSION_ALTER}.${VERSION_BUILD}"
        END
    END
    BLOCK "VarFileInfo"
    BEGIN
        VALUE "Translation", 0x409, 1200
    END
END

IDI_ICON ICON ${CMAKE_SOURCE_DIR}/app/app.ico